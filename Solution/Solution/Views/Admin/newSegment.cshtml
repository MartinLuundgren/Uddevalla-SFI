
@{
    ViewBag.Title = "newSegment";
}

<h2>Create new Segment</h2>

<div class="container">

    <form>
        <div class="form-group">
            <label for="segmentName">Namn på segment:</label>
            <input type="text" class="form-control" id="segmentName">
        </div>
        <div class="form-group">
            <label for="segmentUrl">Länk:</label>
            <input type="text" class="form-control" id="segmentUrl">
        </div>
    </form>
    <button type="button" class="btn btn-success">Spara Segment</button>
</div>




